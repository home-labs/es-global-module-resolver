{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2020",
    "target": "ES2015",
    "skipLibCheck": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    // "rootDir": "./",
    "baseUrl": "./",
    "sourceMap": true
    , "paths": {
      "@actjs.on/es-loading-resolver": [
        "dist/@actjs.on/es-loading-resolver"
      ],
      "@actjs.on/es-loading-resolver/*": [
        "dist/@actjs.on/es-loading-resolver/*"
      ],
      "@actjs.on/es-loading-resolver/web": [
        "dist/@actjs.on/es-loading-resolver/web"
      ],
      "@actjs.on/es-loading-resolver/web/*": [
        "dist/@actjs.on/es-loading-resolver/web/*"
      ]
    }
  }
}
